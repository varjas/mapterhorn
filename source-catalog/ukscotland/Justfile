# Source preparation pipeline to be run from mapterhorn/pipelines folder
[no-cd]
default:
    uv run python source_download.py ukscotland
    uv run python source_to_cog.py ukscotland
    uv run python source_bounds.py ukscotland
    uv run python source_polygonize.py ukscotland 32
    uv run python source_create_tarball.py ukscotland