# Source preparation pipeline to be run from mapterhorn/pipelines folder

[no-cd]
default:
    uv run python source_download.py au5f
    uv run python source_unzip.py au5f
    uv run python source_bounds.py au5f
    uv run python source_polygonize.py au5f 32
    uv run python source_create_tarball.py au5f
